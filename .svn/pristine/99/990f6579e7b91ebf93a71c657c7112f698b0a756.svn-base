#import "AlViewLayout.h"
#import "AIUIView.h"

@implementation AIUIView

@synthesize view;

- (id) initWithUIView : (UIView*) wrappedView
{
    self = [super init];
    if (nil != self)
    {
        self.view = wrappedView;
    }
    return self;
}

- (void) doLayout
{
    view.frame = _alViewLayout->getRect();
}

@end
